            @if (Model.Count > 0) {
                <table class="tables">
                    <thead>
                        <tr>
                            <th>市区</th>
                            <th>街道1</th>
                            <th>街道2</th>
                            <th>小区</th>
                            <th>室/厅/厨/卫</th>
                            <th>面积</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model) {
                            <tr>
                                <td>@item.市区</td>
                                <td>@item.街道1</td>
                                <td>@item.街道2</td>
                                <td>@item.小区</td>
                                <td>@item.室厅厨卫</td>
                                <td>@item.面积</td>
                            </tr>
                        }
